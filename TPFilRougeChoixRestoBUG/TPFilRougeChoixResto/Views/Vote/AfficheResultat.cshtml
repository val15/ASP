@model List<TPFilRougeChoixResto.Models.Resultats>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AfficheResultat</title>
</head>
<body>
<div>
    <h1>RESULTAT : </h1>
</div>
<div>
    <table>
        <tr>
            <th>Nom</th>
            <th>Téléphone</th>
            <th>Nombre de vote</th>
        </tr>
        @foreach (var resto in Model)
        {
        <tr>
            <td>@resto.Nom</td>
            <td>@resto.Telephone</td>
            <td>@resto.NombreDeVotes</td>
        </tr>
        }
    </table>

</div>
</body>
</html>
